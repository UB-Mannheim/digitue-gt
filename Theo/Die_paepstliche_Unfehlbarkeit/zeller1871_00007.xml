        <Coords points="46,1408 42,1365 1355,1357 1361,1400 1359,1420 46,1424"/>
        <Baseline points="46,1408 1361,1400"/>
        <TextEquiv>
          <Unicode>nehmen, und die franzöſiſche Republik (oder was bis dahin aus ihr ge⸗</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_31676db7" >
        <Coords points="48,1464 38,1424 48,1412 50,1412 50,1410 50,1410 52,1410 52,1410 52,1410 54,1410 54,1410 56,1410 288,1418 305,1408 307,1408 307,1408 307,1408 309,1408 309,1408 311,1408 529,1416 531,1416 531,1416 569,1406 571,1406 571,1406 573,1406 1359,1406 1363,1454 1361,1480 48,1484"/>
