      <TextLine id="eSc_line_2c8d5d85" >
        <Coords points="30,206 32,152 1392,154 1392,232 1388,243 26,217"/>
        <Baseline points="30,206 1392,232"/>
        <TextEquiv>
          <Unicode>die urſprügliche Vollkommenheit des Menſchen ſo hoch, um ihn zugleich daran</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_e92b6add" >
        <Coords points="30,261 30,215 1390,239 1392,284 1388,295 26,276"/>
        <Baseline points="30,261 1392,284"/>
        <TextEquiv>
          <Unicode>zu erinnern, wie unendlich weit er ſich von ſeinem Ideale entfernt hat. Da</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_b4ee9efe" >
        <Coords points="30,313 30,261 1388,291 1390,339 1386,350 26,326"/>
        <Baseline points="30,313 1390,339"/>
