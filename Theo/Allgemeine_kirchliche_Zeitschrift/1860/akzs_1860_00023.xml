      <TextLine id="eSc_line_75b1667c" >
        <Coords points="30,367 30,319 1383,343 1386,391 1381,402 26,378"/>
        <Baseline points="30,367 1386,391"/>
        <TextEquiv>
          <Unicode>Tugend; das Alles vermag den ſcharfen Blick der Selbſtprüfung nicht zu er⸗</Unicode>
