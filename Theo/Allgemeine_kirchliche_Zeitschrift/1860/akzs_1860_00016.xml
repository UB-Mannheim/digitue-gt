        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_9848b904" >
        <Coords points="63,1322 63,1276 1424,1296 1427,1341 1422,1357 58,1335"/>
        <Baseline points="63,1322 1427,1341"/>
