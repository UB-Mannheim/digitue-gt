        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_e5bbe2c8" >
        <Coords points="63,1270 63,1220 358,1233 432,1224 439,1224 439,1224 441,1224 441,1224 448,1224 511,1235 537,1226 537,1226 537,1226 539,1226 539,1226 541,1226 550,1226 815,1239 1424,1239 1427,1289 1422,1298 60,1278"/>
        <Baseline points="63,1270 1427,1289"/>
