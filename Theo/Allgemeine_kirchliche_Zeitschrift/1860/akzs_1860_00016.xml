        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_42d957ce" >
        <Coords points="56,2070 56,2022 1416,2044 1418,2090 1413,2103 52,2092"/>
        <Baseline points="56,2070 1418,2090"/>
        <TextEquiv>
