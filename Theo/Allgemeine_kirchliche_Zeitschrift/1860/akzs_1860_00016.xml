      <TextLine id="eSc_line_e660bd56" >
        <Coords points="150,1002 150,956 1427,974 1427,1020 1422,1035 145,1024"/>
        <Baseline points="150,1002 1427,1020"/>
        <TextEquiv>
          <Unicode>Daß der Schwerpunkt des Katholicismus in dieſem Augen⸗</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_e5373641" >
        <Coords points="65,1057 65,1009 1424,1030 1427,1072 1422,1089 60,1078"/>
        <Baseline points="65,1057 1427,1072"/>
        <TextEquiv>
          <Unicode>blicke nach Deutſchland verlegt werden ſoll: das ſteht mit dem</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_3b3a400e" >
        <Coords points="63,1111 63,1067 1422,1085 1424,1128 1420,1141 58,1128"/>
        <Baseline points="63,1111 1424,1128"/>
        <TextEquiv>
          <Unicode>religiöſen Berufe des deutſchen Volkes in einem viel engeren Zuſammenhange,</Unicode>
