      <TextLine id="eSc_line_2f33015c" >
        <Coords points="58,1857 58,1811 1420,1829 1420,1874 1416,1887 54,1877"/>
        <Baseline points="58,1857 1420,1874"/>
        <TextEquiv>
          <Unicode>giöſe Beruf und Charakter des deutſchen Volkes nicht in dem römiſchen Katho⸗</Unicode>
        </TextEquiv>
      </TextLine>
      <TextLine id="eSc_line_c7137efc" >
        <Coords points="58,1911 58,1863 1418,1883 1420,1929 1416,1942 54,1927"/>
        <Baseline points="58,1911 1420,1929"/>
        <TextEquiv>
          <Unicode>licismus ſeinen wahrſten und innerſten Ausdruck zu finden vermag. Die Na⸗</Unicode>
